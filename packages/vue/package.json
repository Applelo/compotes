{
  "name": "@compotes/vue",
  "type": "module",
  "version": "0.10.0",
  "packageManager": "pnpm@9.11.0",
  "description": "Components library focused on accessibility/customization",
  "author": "Applelo",
  "license": "MIT",
  "homepage": "https://github.com/Applelo/blottie",
  "repository": {
    "type": "git",
    "url": "https://github.com/Applelo/blottie"
  },
  "bugs": "https://github.com/Applelo/blottie/issues",
  "keywords": [
    "components",
    "accessible",
    "accessibility",
    "customize",
    "component",
    "vanilla",
    "typescript",
    "typings"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/compotes.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/compotes.umd.cjs"
      }
    }
  },
  "main": "./dist/compotes.umd.cjs",
  "module": "./dist/compotes.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "compotes": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue": "^3.5.10",
    "vue-tsc": "^2.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
